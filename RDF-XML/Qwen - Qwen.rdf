            </rdf:Description>
        </ex:has_results>
        <ex:has_results>
            <rdf:Description>
                <ex:links_to rdf:resource="http://example.org/Result/Silicon_refinement"/>
                <ex:has_value>Inclusions reduced to 5-10 µm</ex:has_value>
            </rdf:Description>
        </ex:has_results>
    </rdf:Description>

