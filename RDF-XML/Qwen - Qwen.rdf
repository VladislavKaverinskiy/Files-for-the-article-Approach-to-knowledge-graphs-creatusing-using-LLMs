        </ex:studies_property>
        <ex:studies_property>
            <rdf:Description>
                <ex:links_to rdf:resource="http://example.org/Material_property/Tensile_strength"/>
                <ex:has_value>Up to 333.3 MPa</ex:has_value>
            </rdf:Description>
        </ex:studies_property>
        <ex:uses_method>
            <rdf:Description>
                <ex:links_to rdf:resource="http://example.org/Research_method/Tensile_testing"/>
