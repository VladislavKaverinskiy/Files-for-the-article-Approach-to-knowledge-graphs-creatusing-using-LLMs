                <ex:links_to rdf:resource="http://example.org/Article/About_Al-Si_Alloys_Structure_Features..."/>
                <ex:has_value>Core subject</ex:has_value>
            </rdf:Description>
        </ex:includes_terms>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/Key_word/Deformation_heat_processing">
        <rdf:type rdf:resource="http://example.org/Key_word"/>
        <ex:name>Deformation Heat Processing</ex:name>
        <ex:description>Technique combining hot plastic deformation, intermediate cooling, and annealing to refine microstructure</ex:description>
        <ex:develops_technology>
            <rdf:Description>
                <ex:links_to rdf:resource="http://example.org/Article/About_Al-Si_Alloys_Structure_Features..."/>
