    <rdf:Description rdf:about="http://example.org/Key_word/Al-Si_alloys">
        <rdf:type rdf:resource="http://example.org/Key_word"/>
        <ex:name>Al-Si Alloys</ex:name>
        <ex:description>Eutectic aluminum-silicon alloys known for low density, corrosion resistance, and brittleness in cast state</ex:description>
