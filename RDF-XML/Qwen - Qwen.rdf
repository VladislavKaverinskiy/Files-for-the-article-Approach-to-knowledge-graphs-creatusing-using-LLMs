            </rdf:Description>
        </ex:develops_technology>
        <ex:includes_terms>
            <rdf:Description>
                <ex:links_to rdf:resource="http://example.org/Article/About_Al-Si_Alloys_Structure_Features..."/>
                <ex:has_value>Core methodology</ex:has_value>
            </rdf:Description>
        </ex:includes_terms>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/Key_word/Work_hardening">
        <rdf:type rdf:resource="http://example.org/Key_word"/>
        <ex:name>Work Hardening</ex:name>
        <ex:description>Cold deformation process to enhance strength through dislocation density increase</ex:description>
